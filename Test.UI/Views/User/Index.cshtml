@model List<Test.UI.Models.ViewModels.User.ShowUserViewModel>

<h1>List of Users</h1>

<table class="table table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Family</th>
            <th>Phone Number</th>
            <th>Address</th>
            <th>City</th>
            <th>Province</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Name</td>
                <td>@user.Family</td>
                <td>@user.PhoneNumber</td>
                <td>@user.Address</td>
                <td>@user.CityName</td>
                <td>@user.ProvinceName</td>
            </tr>
        }
    </tbody>
</table>

